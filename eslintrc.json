{
    "extends": ["airbnb-base"],
    "rules": {
        "no-use-before-define": [
            "error",
            {
                "functions": false
            }
        ],
        "prefer-destructuring": ["error", {
            "array": false,
            "object": true
          }, {
            "enforceForRenamedProperties": false
          }],
        "no-restricted-syntax": ["off","ForOfStatement,ForSatement,ForInStatement","SwitchStatement"],
        "prefer-promise-reject-errors": "off",
        "no-console": "off",
        "no-plusplus": "off"
    }
}
